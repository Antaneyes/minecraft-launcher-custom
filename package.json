{
  "name": "ombicraft-launcher",
  "productName": "OmbiCraft Launcher",
  "version": "1.0.21",
  "description": "OmbiCraft Launcher",
  "author": "josalro",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "test": "jest",
    "lint": "eslint .",
    "release": "node maintenance/release.js",
    "update-server": "node maintenance/update_server.js"
  },
  "build": {
    "appId": "com.ombicraft.launcher",
    "publish": [
      {
        "provider": "github",
        "owner": "Antaneyes",
        "repo": "minecraft-launcher-custom"
      }
    ],
    "win": {
      "target": "nsis",
      "artifactName": "OmbiCraft-Launcher-Setup-${version}.${ext}"
    },
    "files": [
      "ui/**/*",
      "utils/**/*",
      "index.js",
      "package.json"
    ]
  },
  "license": "ISC",
  "main": "index.js",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.13.2",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "fs-extra": "^11.3.2",
    "minecraft-launcher-core": "^3.18.2",
    "msmc": "^5.0.5"
  },
  "devDependencies": {
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^30.2.0"
  }
}
